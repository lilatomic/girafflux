---g
from "example-bucket"
|@ start -1h
|$ "cpu"
|% "usage_system"
|? cmp.eq(a: _.cpu, b: "cpu-total")
---f
from(bucket: "example-bucket")
    |> range(start: -1h)
    |> filter(
        fn: (r) => r._measurement == "cpu" and r._field == "usage_system" and r.cpu == "cpu-total",
    )
---u
from(bucket: "example-bucket")
|> range(start: -1h)
|> filter(
fn: (r) =>
r["_measurement"] == "cpu"
)
|> filter(
fn: (r) =>
r["_field"] == "usage_system"
)
|> filter(
fn: (r) =>
r.cpu == "cpu-total"
)
